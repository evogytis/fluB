	Issn = {1674-{800X}, 1674-8018},
	Journal = {Protein \& Cell},
	Keywords = {Biochemistry, general, Cell Biology, developmental biology, Human Genetics, Influenza virus, pandemic influenza, pandemics control, Protein Science, Stem Cells},
	Language = {en},
	Month = jan,
	Number = {1},
