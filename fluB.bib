	Issn = {0304-8608, 1432-8798},
	Journal = {Archives of Virology},
	Language = {en},
	Month = jul,
	Number = {8},
	Pages = {1569--1583},
	Shorttitle = {Evolutionary characteristics of influenza B virus since its first isolation in 1940},
	Title = {Evolutionary characteristics of influenza B virus since its first isolation in 1940: dynamic circulation of deletion and insertion mechanism},
