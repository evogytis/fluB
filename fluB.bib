	Issn = {0304-8608, 1432-8798},
	Journal = {Archives of Virology},
	Keywords = {Infectious Diseases, Medical Microbiology, Virology},
	Language = {en},
	Month = feb,
	Number = {2},
	Pages = {415--422},
	Title = {Exploration of the emergence of the Victoria lineage of influenza B virus},
