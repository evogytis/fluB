	File = {Full Text PDF:/Users/admin/Library/Application Support/Firefox/Profiles/tit72ymn.default/zotero/storage/8VCE7TMF/Hiromoto et al. - 2000 - Phylogenetic analysis of the three polymerase gene.pdf:application/pdf;Snapshot:/Users/admin/Library/Application Support/Firefox/Profiles/tit72ymn.default/zotero/storage/VRV4BBM2/929.html:text/html},
	Issn = {0022-1317, 1465-2099},
