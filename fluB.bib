	Language = {en},
	Month = apr,
	Note = {{PMID:} 10725418},
	Number = {4},
	Pages = {929--937},
	Title = {Phylogenetic analysis of the three polymerase genes ({PB1}, {PB2} and {PA)} of influenza B virus},
